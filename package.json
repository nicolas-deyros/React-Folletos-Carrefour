{
  "name": "folletos-carrefour",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm-run-all --parallel init-catalogs fetch-catalogs vite-build",
    "fetch-catalogs": "node --experimental-modules src/fetch-catalogs.mjs",
    "serve": "serve dist",
    "dev": "npm-run-all --parallel init-catalogs  fetch-catalogs- dev-server",
    "fetch-catalogs-": "nodemon --watch src --exec \"node --experimental-modules src/fetch-catalogs.mjs\"",
    "dev-server": "vite",
    "init-catalogs": "nodemon --watch src --exec \"node --experimental-modules src/initCatalogs.mjs\"",
    "vite-build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "framer-motion": "^10.9.2",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-alice-carousel": "^2.7.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0",
    "vite": "^4.2.0"
  }
}