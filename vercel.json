{
  "functions": {
    "api/update-catalogs.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "serverless": {
    "function": {
      "timeout": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron.js",
      "schedule": "00 12 * * *"
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "../api/cron.js"
    }
  ]
}