{
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "command": "npm run build",
    "src": "src/app.js"
  },
  "routes": [
    {
      "src": "/api/catalogs",
      "dest": "./api/catalogs.js"
    }
  ],
  "functions": {
    "api/**/*": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}